{
  "name": "@windingtree/wt-ui-react",
  "version": "0.10.0",
  "description": "React framework",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run build:clean && npm run build:lib && npm run build:flow",
    "build:clean": "rimraf lib",
    "build:lib": "babel -d lib src --copy-files --ignore tests,layouts,app",
    "build:flow": "flow-copy-source -v src lib",
    "coverage": "cat coverage/lcov.info | coveralls",
    "flow": "flow",
    "lint": "eslint src && eslint stories && eslint test && stylelint src",
    "lint:fix": "eslint --fix src && eslint --fix stories && eslint --fix test && stylelint --fix src",
    "test": "rimraf coverage && jest --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs"
  },
  "keywords": [
    "React",
    "Winding",
    "Tree",
    "framework"
  ],
  "author": "Winding Tree devs",
  "license": "Apache-2.0",
  "jest": {
    "verbose": true,
    "setupTestFrameworkScriptFile": "<rootDir>test/utils/setupTests.js",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "dependencies": {
    "@windingtree/wt-ui": "^1.1.1",
    "classnames": "^2.2.6",
    "react": "^16.5.2",
    "react-bootstrap": "^1.0.0-beta.1",
    "react-dom": "^16.5.2",
    "react-slick": "^0.23.2",
    "react-transition-group": "^2.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^4.0.2",
    "@storybook/addon-info": "^3.4.11",
    "@storybook/addon-knobs": "^4.0.2",
    "@storybook/addon-links": "^4.0.2",
    "@storybook/addon-options": "^4.0.6",
    "@storybook/addons": "^4.0.2",
    "@storybook/react": "^4.0.2",
    "autoprefixer": "^9.1.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-react-docgen": "^2.0.0",
    "babel-runtime": "^6.26.0",
    "clean-webpack-plugin": "^1.0.0",
    "coveralls": "^3.0.2",
    "css-loader": "^1.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-flowtype": "^2.50.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.85.0",
    "flow-copy-source": "^2.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "node-sass": "^4.9.3",
    "precss": "^3.1.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "sinon": "^6.3.5",
    "slick-carousel": "^1.8.1",
    "style-loader": "^0.23.0",
    "stylelint": "^9.6.0",
    "stylelint-config-sass-guidelines": "^5.2.0",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
